{
  "deploy": {
    "startCommand": "python -m uvicorn api_server:app --host 0.0.0.0 --port $PORT",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "environments": {
    "production": {
      "variables": {
        "PYTHON_VERSION": "3.11",
        "PORT": "8000",
        "ENVIRONMENT": "production"
      }
    }
  }
}